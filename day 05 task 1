a = int(input("Please Enter The 1st Num :- "))
b = int(input("Please Enter The 2nd NUm :- "))
print(a,"+",b,"=",a+b)
print(a,"*",b,"=",a*b)
print(a,"-",b,"=",a-b)
print(a,"/",b,"=",a/b)
print(a,"%",b,"=",a%b)
